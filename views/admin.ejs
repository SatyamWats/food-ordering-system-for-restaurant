<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <h1>Admin page</h1>
    <h3><%= errorMessage %></h3>
    <% orderList.forEach(currentOrder => { %>
        <div>
            <p>ID: <%= currentOrder._id %></p>
            <% currentOrder.foods.forEach(currentFoodItem => { %>
                <p>Name: <%= currentFoodItem.name %></p>
                <p>Quantity: <%= currentFoodItem.quantity   %></p>
            <% }); %>
            <form action="/delivered" method="POST">
                <button type="submit">Delivered</button>
                <input name="customerID" value=<%=currentOrder._id%> style="display: none"></input>
            </form>
        </div>
    <% }); %>
</body>
</html>